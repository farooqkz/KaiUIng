{
  "name": "kaiuing-inferno",
  "version": "3.3.0",
  "description": "Inferno library for KaiUIng",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "parcel build",
    "lint": "tslint -p ./tsconfig.json",
    "check": "tsc -p ./tsconfig.json --noEmit",
    "test": "yarn run check && yarn run lint"
  },
  "repository": "https://github.com/farooqkz/KaiUIng",
  "author": "Farooq Karimi Zadeh",
  "license": "LGPL-3.0-or-later",
  "private": false,
  "dependencies": {
    "@swc/helpers": "^0.4.14",
    "KaiUI": "npm:kaiuing@^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@parcel/packager-ts": "2.8.3",
    "@parcel/transformer-babel": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "@parcel/transformer-typescript-types": "2.8.3",
    "babel-plugin-inferno": "^6.6.0",
    "classnames": "^2.3.2",
    "inferno": "^8.0.6",
    "inferno-extras": "^8.0.6",
    "parcel": "^2.8.3",
    "ts-plugin-inferno": "^1.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "classnames": "^2.3.2",
    "inferno": "^8.2.2",
    "inferno-extras": "^8.2.2"
  },
  "browserslist": "Firefox 48",
  "targets": {
    "main": {
      "isLibrary": true,
      "source": "src/index.ts",
      "outputFormat": "commonjs",
      "includeNodeModules": {
        "inferno": false,
        "inferno-extras": false,
        "classnames": false
      }
    },
    "module": {
      "isLibrary": true,
      "source": "src/index.ts",
      "outputFormat": "esmodule",
      "includeNodeModules": {
        "inferno": false,
        "inferno-extras": false,
        "classnames": false
      }
    },
    "types": {
      "isLibrary": true,
      "source": "src/index.ts"
    }
  }
}
